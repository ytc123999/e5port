import turtle
# 创建哆啦A梦
doraemon = turtle.Turtle()
doraemon.speed(10)
def draw_eye_white_circle(x):
  doraemon.goto(x, 80)
  doraemon.pendown()
  doraemon.color('black')
  doraemon.begin_fill()
  doraemon.circle(15)
  doraemon.color('white')
  doraemon.end_fill()
def draw_eye_black_circle(x):
  doraemon.goto(x, 90)
  doraemon.color('black')
  doraemon.begin_fill()
  doraemon.begin_fill()
  doraemon.circle(6)
  doraemon.end_fill()
  doraemon.penup()
